{
  "rules": {
    "bingo": {
      "participants": {
        ".read": true,
        ".write": true,
        "$number": {
          ".validate": "newData.hasChildren(['number', 'name', 'phone', 'timestamp'])",
          "number": {
            ".validate": "newData.isNumber() && newData.val() >= 1 && newData.val() <= 50"
          },
          "name": {
            ".validate": "newData.isString() && newData.val().length >= 2 && newData.val().length <= 100"
          },
          "phone": {
            ".validate": "newData.isString() && newData.val().matches(/^[0-9]{10}$/)"
          },
          "timestamp": {
            ".validate": "newData.isString() && newData.val().length > 0"
          },
          "$other": {
            ".validate": false
          }
        }
      }
    },
    "test": {
      ".read": true,
      ".write": true
    },
    "$other": {
      ".read": false,
      ".write": false
    }
  }
}
